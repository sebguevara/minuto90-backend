generator client {
  provider = "prisma-client"
  output   = "../prisma-whoscored/whoscored-client-types"
}

datasource db {
  provider = "postgresql"
}

model BestXI {
  id           Int            @id @default(autoincrement())
  tournamentId Int
  timeframe    String
  startDate    DateTime
  endDate      DateTime
  formation    String         @default("4-4-2")
  createdAt    DateTime       @default(now())
  Tournament   Tournament     @relation(fields: [tournamentId], references: [id], onDelete: Cascade)
  BestXIPlayer BestXIPlayer[]

  @@unique([tournamentId, timeframe, startDate, endDate])
}

model BestXIPlayer {
  id            Int     @id @default(autoincrement())
  bestXIId      Int
  teamId        Int
  playerName    String
  rating        Float
  positionLabel String
  positionIndex Int
  playerId      Int?
  BestXI        BestXI  @relation(fields: [bestXIId], references: [id], onDelete: Cascade)
  Player        Player? @relation(fields: [playerId], references: [id])
  Team          Team    @relation(fields: [teamId], references: [id], onDelete: Cascade)
}

model Country {
  id               Int                @id @default(autoincrement())
  name             String?
  code             String?            @unique
  createdAt        DateTime           @default(now())
  updatedAt        DateTime
  Player           Player[]
  Team             Team[]
  TeamInTournament TeamInTournament[]
  Tournament       Tournament[]
}

model Player {
  id                                                                Int                              @id
  name                                                              String
  countryId                                                         Int?
  createdAt                                                         DateTime                         @default(now())
  updatedAt                                                         DateTime
  minId                                                             Int?
  BestXIPlayer                                                      BestXIPlayer[]
  Country                                                           Country?                         @relation(fields: [countryId], references: [id])
  PlayerTableDefensive                                              PlayerTableDefensive[]
  PlayerTableOffensive                                              PlayerTableOffensive[]
